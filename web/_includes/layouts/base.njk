<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>{{ renderData.title or title or metadata.title }}</title>
    <meta name="Description" content="{{ renderData.description or description or metadata.description }}">
      {# <link rel="alternate" href="{{ metadata.feed.path | url }}" type="application/atom+xml" title="{{ metadata.title }}"> #}
    <link rel="stylesheet" href="public/main.css"/>
  </head>
  <body>
    <header class="pt-10 max-w-7xl mx-auto">
      <a href="/">
      <img class="h-8 w-auto sm:h-10" src="https://tailwindui.com/img/logos/workflow-mark-blue-600.svg">
      Generation with The Coders Guild
      </a>
  </header>

    <main{% if templateClass %} class="{{ templateClass }}"{% endif %}>
      {{ content | safe }}
    </main>
    <footer class="bg-gray-100">
        <p><em>&copy; 2021  <a href="https://www.thecodersguild.org.uk">The Coders Guild</a></em></p>
    </footer>

    <!-- Current page: {{ page.url | url }} -->
  </body>
</html>
